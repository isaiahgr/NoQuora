{
  "name": "NoQuora",
  "description": "Removes Quora results from search.",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "*://*.google.com/*"
  ]
}
